{
    "$schema": "../.sitecore/schemas/ModuleFile.schema.json",
    "namespace": "InitItems",
    "items": {
        "includes": [
            {
                "name": "content-root",
                "path": "/sitecore/content",
                "scope": "singleItem",
                "allowedPushOperations": "createAndUpdate"
            },
            {
                "name": "api-key",
                "path": "/sitecore/system/Settings/Services/API Keys/PLAY",
                "scope": "singleItem"
            },
            {
                "name": "core-fields",
                "path": "/sitecore/system/Field types/PLAY Types",
                "database": "core"
            },
            {
                "name": "image-field-type",
                "path": "/sitecore/system/Field types/Simple Types/Image",
                "scope": "singleItem",
                "database": "core"
            },
            {
                "name": "powershell-scripts-warmup",
                "path": "/sitecore/system/Modules/PowerShell/Script Library/PLAY"
            },
            {
                "name": "languages",
                "path": "/sitecore/system/Languages",
                "scope": "descendantsOnly",
                "rules": [
                    {
                        "path": "/en",
                        "scope": "ignored"
                    }
                ]
            },
            {
                "name": "play-microsite/settings",
                "path": "/sitecore/system/Settings/Feature/PLAY"
            },
            {
                "name": "play-microsite/template",
                "path": "/sitecore/templates/Branches/Feature/PLAY"
            },
            {
                "name": "play-microsite/images",
                "path": "/sitecore/media library/Feature/PLAY"
            },
            {
                "name": "ps1",
                "path": "/sitecore/content/PLAY/playwebsite/home/ps1",
                "scope": "ItemAndDescendants"
            },
            {
                "name": "ps2",
                "path": "/sitecore/content/PLAY/playwebsite/home/ps2",
                "scope": "ItemAndDescendants"
            },
            {
                "name": "ps3",
                "path": "/sitecore/content/PLAY/playwebsite/home/ps3",
                "scope": "ItemAndDescendants"
            },
            {
                "name": "ps4",
                "path": "/sitecore/content/PLAY/playwebsite/home/ps4",
                "scope": "ItemAndDescendants"
            },
            {
                "name": "ps5",
                "path": "/sitecore/content/PLAY/playwebsite/home/ps5",
                "scope": "ItemAndDescendants"
            },
            {
                "name": "ps6",
                "path": "/sitecore/content/PLAY/playwebsite/home/ps6",
                "scope": "ItemAndDescendants"
            },
            {
                "name": "ps7",
                "path": "/sitecore/content/PLAY/playwebsite/home/ps7",
                "scope": "ItemAndDescendants"
            },
            {
                "name": "ps8",
                "path": "/sitecore/content/PLAY/playwebsite/home/ps8",
                "scope": "ItemAndDescendants"
            },
            {
                "name": "ps9",
                "path": "/sitecore/content/PLAY/playwebsite/home/ps9",
                "scope": "ItemAndDescendants"
            },
            {
                "name": "ps10",
                "path": "/sitecore/content/PLAY/playwebsite/home/ps10",
                "scope": "ItemAndDescendants"
            }
        ]
    }
}